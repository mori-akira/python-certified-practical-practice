    "# ジェネレータ\n",
    "def generator(limit):\n",
    "    ret = 1\n",
    "    for _ in range(limit):\n",
    "        yield ret\n",
    "        ret = ret * 2\n",
    "\n",
    "for i in generator(10):\n",
    "    print(i)"
